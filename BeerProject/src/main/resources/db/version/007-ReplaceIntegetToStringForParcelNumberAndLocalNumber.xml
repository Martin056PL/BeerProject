<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

        <changeSet author="Kamil Wawer" id="10">
            <dropColumn columnName="parcel_number"
                        tableName="address"/>
        </changeSet>

        <changeSet author="Kamil Wawer" id="11">
            <addColumn tableName="address">
                <column name="parcel_number" type="varchar(255)"/>
            </addColumn>
        </changeSet>

    <changeSet author="Kamil Wawer" id="12">
        <dropColumn columnName="local_number"
                    tableName="address"/>
    </changeSet>

    <changeSet author="Kamil Wawer" id="13">
        <addColumn tableName="address">
            <column name="local_number" type="varchar(255)"/>
        </addColumn>
    </changeSet>
</databaseChangeLog>
