    <changeSet id="17" author="Iwona">
        <loadData file="classpath:/db/samples/009-imagesSamples.csv"
                  tableName="brewery">
            <column name="brewery_image" type="TEXT" header="brewery_image"/>
        </loadData>
    </changeSet>
</databaseChangeLog>
